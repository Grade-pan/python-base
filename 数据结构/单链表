class Node:
    def __init__(self, dataval=None):
        self.dataval = dataval
        self.nextval = None


class SLinkedList:
    def __init__(self, headval=None):
        self.headval = None

    def listprint(self):
        printval = self.headval
        while printval:
            print(printval.dataval)
            printval = printval.nextval


list = SLinkedList()
list.headval = Node('Mon')
e2 = Node('Tue')
e3 = Node('Wed')

list.headval.nextval = e2
e2.nextval = e3

list.listprint()